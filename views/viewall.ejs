<% include partials/header %>

<h1>Home PAge</h1>

<div class="container">

<table class="table">
    <thead>
      <tr>
        <th>Event Name</th>
        <th>Organiser Name</th>
        <th>Location</th>
        <th>Date</th>
      </tr>
    </thead>
    <tbody>
     <% datas.forEach(function(data){ %>
      <tr>
        <td><%= data.ename %></td>
        <td><%= data.oname %></td>
        <td><%= data.location %></td>
        <td><%= data.date %></td>
      </tr>
<% }); %>

</div>

<% include partials/footer %>